//	@Column(name = "USER_PASSWORD")
	private String password;
	
//	@Column(name = "USER_RANK")
	protected int rank;
	//add the setting below, above is for 5-4 mvp
	
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	public String getUsername() {
		return username;
	}
	public void setUsername(String username) {
